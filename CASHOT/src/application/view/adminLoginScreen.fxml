<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="content" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AdminLoginController">
	<children>
		<VBox maxHeight="800.0" maxWidth="800.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
		   <children>
		      <Pane maxHeight="800.0" maxWidth="800.0" prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: #e82e15;">
		         <children>
		            <Label layoutX="50.0" layoutY="41.0" prefHeight="76.0" prefWidth="701.0" text="Please enter your login information:" textAlignment="CENTER">
		               <font>
		                  <Font size="44.0" />
		               </font>
		            </Label>
		            <Label layoutX="744.0" layoutY="776.0" text="Cashot" />
                 <VBox alignment="CENTER" layoutX="249.0" layoutY="245.0" prefHeight="181.0" prefWidth="274.0" spacing="10.0">
                     <children>
                        <Label text="Username (Example: admin)" textAlignment="CENTER" />
                        <TextField fx:id="adminUsrName" onAction="#handle" onKeyPressed="#setTextToBlank" prefHeight="41.0" prefWidth="274.0" />
                        <Label text="Password (Example: admin)" />
                        <PasswordField fx:id="adminUsrPw" onAction="#handle" onKeyPressed="#setTextToBlank" />
                        <HBox alignment="CENTER" prefHeight="68.0" prefWidth="274.0" spacing="50.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#loadMain" prefHeight="25.0" prefWidth="80.0" text="Go Back">
                                 <font>
                                    <Font size="12.0" />
                                 </font></Button>
                              <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#handle" prefHeight="25.0" prefWidth="80.0" text="Login">
                                 <font>
                                    <Font size="12.0" />
                                 </font></Button>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <Label fx:id="logInIncorrect" layoutX="300.0" layoutY="477.0" prefHeight="64.0" prefWidth="173.0" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                     <font>
                        <Font size="12.0" />
                     </font></Label>
		         </children>
		      </Pane>
		   </children>
		</VBox>
	</children>
</AnchorPane>
